<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yixuan.yh.admin.mapper.AdminPermissionMapper">

    <insert id="insertAdminPermissions">
        insert into admin_permission (admin_id, permission_code) values
        <foreach collection="permissionList" item="permissionCode" separator=",">
            (#{adminId}, #{permissionCode})
        </foreach>
    </insert>
</mapper>