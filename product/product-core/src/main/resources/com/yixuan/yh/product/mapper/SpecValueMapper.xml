<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yixuan.yh.product.mapper.SpecValueMapper">

    <delete id="deleteBatch">
        delete from spec_value where value_id in
        <foreach collection="valueIdList" item="valueId" open="(" close=")" separator=",">
            #{valueId}
        </foreach>
    </delete>
</mapper>